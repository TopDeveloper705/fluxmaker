  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-1": "^6.5.0",
    "chai": "^3.5.0",
