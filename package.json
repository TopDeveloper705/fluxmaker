{
  "name": "fluxmaker",
  "version": "1.0.0",
  "description": "Isomorphic React-Flux application framework.",
  "main": "index.js",
  "scripts": {
    "unit": "mocha \"spec/unit/**/*.js\" --compilers js:babel-register --require babel-polyfill",
    "integration": "cd spec/integration && npm test",
    "test": "npm run unit && npm run integration",
    "compile": "babel src --out-dir .",
    "watch": "babel src --watch --out-dir .",
    "lint": "gulp lint",
    "prepublish": "npm run compile"
  },
  "keywords": [
    "flux",
    "react",
    "isomorphic"
  ],
  "author": "Talysson <talyssonoc@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "bluebird": "^3.3.4",
    "body-parser": "^1.15.0",
    "compression": "^1.6.1",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "factory-girl": "^3.0.0",
    "factory-girl-sequelize": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "gulp-nodemon": "^2.0.6",
    "interop-require": "^1.0.0",
    "react-hot-loader": "^1.3.0",
    "run-sequence": "^1.1.5",
    "sequelize": "^3.20.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
