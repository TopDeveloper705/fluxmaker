{
  "name": "fluxmaker",
  "version": "1.0.0",
  "description": "Isomorphic React-Flux application framework.",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha \"test/unit/**/*.js\"",
    "test:integration": "cd test/integration && npm test",
    "compile": "babel src --out-dir .",
    "watch": "babel src --watch --out-dir .",
    "lint": "eslint \"src/**/*.js\" \"test/unit/**/*.js\"",
    "prepublish": "npm run compile"
  },
  "bin": {
    "fmk": "./bin/fmk.js"
  },
  "keywords": [
    "flux",
    "react",
    "isomorphic"
  ],
  "author": "Talysson <talyssonoc@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "bluebird": "^3.3.4",
    "body-parser": "^1.15.0",
    "compression": "^1.6.1",
    "debug": "^2.2.0",
    "eventemitter2": "^1.0.0",
    "express": "^4.13.4",
    "factory-girl": "^3.0.0",
    "factory-girl-sequelize": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.0.6",
    "gulp-sass": "^2.3.1",
    "interop-require": "^1.0.0",
    "interpret": "^1.0.0",
    "liftoff": "^2.2.1",
    "react-hot-loader": "^1.3.0",
    "resolve": "^1.1.7",
    "sequelize": "^3.20.0",
    "supertest": "^1.2.0",
    "supertest-as-promised": "^3.1.0",
    "v8flags": "^2.0.11",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
    "react": "^0.14.8"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-1": "^6.5.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  }
}
